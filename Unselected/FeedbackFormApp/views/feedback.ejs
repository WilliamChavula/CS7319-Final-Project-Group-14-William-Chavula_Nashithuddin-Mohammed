<!DOCTYPE html>
<html>

<head>
    <title>Feedback Form</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">
    <link rel="stylesheet" href="https://cdn.datatables.net/1.10.2/css/jquery.dataTables.min.css">
    <link rel="stylesheet" href="stylesheets/home.css">
</head>

<body>
    <nav class="navbar navbar-expand-lg bg-body-tertiary">
        <div class="container-fluid">
            <a class="navbar-brand" href="/">FeedbackForm</a>
            <div id="feedbackDiv">
                <a class="btn btn-primary" href="/feedback" role="button" id="feedbackLink">Feedback Form</a>
            </div>
            <div id="logoutDiv">
                <a class="btn btn-primary" href="/logout" role="button" id="logoutLink">Logout</a>
            </div>
            <div id="loginDiv">
                <a class="btn btn-primary me-2" href="/login" role="button">Login</a>
                <a class="btn btn-primary" href="/register" role="button">Register</a>
            </div>
        </div>
    </nav>
    <div class="container">
        <div class="card feedbackcard" style="width: 18rem;">
            <div class="card-body">
                <h5 class="card-title">Feedback</h5>
                <table class="table" id="feedbackTable">
                    <thead>
                        <tr>
                            <th scope="col">SMU ID</th>
                            <th scope="col">Professor Name</th>
                            <th scope="col">Course Title</th>
                            <th scope="col">Course Code</th>
                            <th scope="col">Feedback</th>
                            <th scope="col">Timestamp</th>
                        </tr>
                    </thead>
                    <tbody>
                        <% for(var i=0; i<data.feedbackData.length; i++) { %>
                            <tr>
                                <td>
                                    <%= data.feedbackData[i].smuid %>
                                </td>
                                <td>
                                    <%= data.feedbackData[i].profName %>
                                </td>
                                <td>
                                    <%= data.feedbackData[i].courseTitle %>
                                </td>
                                <td>
                                    <%= data.feedbackData[i].courseCode %>
                                </td>
                                <td>
                                    <%= data.feedbackData[i].feedback %>
                                </td>
                                <td>
                                    <%= data.feedbackData[i].updated %>
                                </td>
                            </tr>
                            <% } %>
                    </tbody>
                </table>
            </div>
        </div>
    </div>

    <!-- <script>
        var loggedIn = localStorage.getItem("loggedIn")
    </script> -->
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.1/dist/umd/popper.min.js"
        integrity="sha384-9/reFTGAW83EW2RDu2S0VKaIzap3H66lZH81PoYlFhbGU+6BZp6G7niu735Sk7lN"
        crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js"
        integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL"
        crossorigin="anonymous"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.7.1/jquery.min.js"></script>
    <script type="text/javascript" src="https://cdn.datatables.net/1.10.2/js/jquery.dataTables.min.js"></script>
    <script src="javascripts/feedbackcontroller.js"></script>
</body>

</html>